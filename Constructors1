import java.lang.reflect.Constructor;

public class Box {

	double width;
	double height;
	double depth;
Box (double width, double height,double depth)
{          // parameter constructors 
      this.width=width;

      this.height=height;
     this.depth=depth;
}



   double volume()
{
		 return width*height*depth;

}



public static void main (String args [])
{       double vol;
	   Box myBox1= new Box(10,10,20);
	   Box myBox2= new Box(10,30,40);
	   Box myBox3= new Box(10,50,40);
	   Box myBox4= new Box(40,50,60);
	  vol=myBox1.volume();
	  vol=myBox2.volume();
	  vol=myBox3.volume();
	  vol=myBox4.volume();
	  System.out.println("myBox1 volume :"+vol);
	  System.out.println("myBox2 volume :"+vol);
	  System.out.println("myBox3 volume : "+vol);
	  System.out.println("myBox4 volume :"+vol);
	


}
}

