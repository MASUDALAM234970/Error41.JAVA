 class Alam{

        void display()
        {
            System.out.println(" Hi i am not static method");

        }
      static  void display1()
        {
            System.out.println(" Hi i am static method");

        }

      public static void main(String[] args) {
        Alam s= new Alam();
        s.display();
        System.out.println("__________________________");
        Alam.display1();

      }

 }
